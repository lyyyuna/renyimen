{
  "name": "renyimen",
  "version": "1.0.0",
  "description": "任意门智能导航系统 - Electron桌面应用",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "keywords": [
    "electron",
    "vue",
    "amap",
    "navigation",
    "claude",
    "mcp"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "build": {
    "appId": "com.renyimen.app",
    "productName": "任意门",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "electron/**/*",
      "dist/**/*"
    ],
    "mac": {
      "category": "public.app-category.navigation"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
